-- Scripts:

local function ULOYH_fake_script() -- label.LocalScript 
	local script = Instance.new('LocalScript', label)

	-- Colored-background
	while true do
		for a = 1,360,1 do
			wait(0.1)
			script.Parent.BackgroundColor3 = Color3.fromHSV(a/360,1,1)
		end
	end
end
coroutine.wrap(ULOYH_fake_script)()
